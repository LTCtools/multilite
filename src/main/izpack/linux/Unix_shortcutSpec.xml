<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>

<shortcuts>

  <programGroup defaultName="MultiLite"
  location="applications"/>

   <shortcut
     name="MultiLite"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="/usr/bin/java"
     commandLine="-jar $INSTALL_PATH/multilite-exe.jar"
     initialState="normal"
     iconFile="$INSTALL_PATH/multilite48.png"
     workingDirectory="$INSTALL_PATH"
     type="Application"
     encoding="UTF-8"
     description="MultiLite">
   </shortcut>
   <shortcut
     name="Uninstall MultiLite"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="no"
     startup="no"
     target="/usr/bin/java"
     commandLine="-jar $INSTALL_PATH/Uninstaller/uninstaller.jar"
     initialState="normal"
     workingDirectory="$INSTALL_PATH"
     type="Application"
     encoding="UTF-8"
     description="Uninstall MultiLite">
   </shortcut>
</shortcuts>

